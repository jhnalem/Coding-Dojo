<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <title>Dojo Survey</title>

        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.8/css/materialize.min.css">
        <link rel="stylesheet" href="~/css/styles.css"/>
    </head>

    <body>
        <main class="section">
            <div class="container">
                <div class="row">
                    <h1 class="center-align">Dojo Survey Results</h1>
                </div>

                <div class="row">
                    <div class="col s6 right-align"><strong>Name:</strong></div>
                    <div class="col s6">@ViewBag.Name</div>
                </div>

                <div class="row">
                    <div class="col s6 right-align"><strong>Location:</strong></div>
                    <div class="col s6">@ViewBag.Location</div>
                </div>

                <div class="row">
                    <div class="col s6 right-align"><strong>Favorite Language:</strong></div>
                    <div class="col s6">@ViewBag.Language</div>
                </div>
            </div>
        </main>

        <script src="https://code.jquery.com/jquery-3.1.1.min.js"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.8/js/materialize.min.js"></script>
        <script src="~/js/script.js"></script>
    </body>
</html>